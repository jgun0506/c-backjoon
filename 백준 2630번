#define _CRT_SECURE_NO_WARNINGS
#pragma comment(linker, "/STACK:28388608")   // 8MB 스택메모리늘려줌
#include <stdio.h>
#include <stdlib.h>    // srand, rand 함수가 선언된 헤더 파일
#include <string.h>
#include <stdbool.h>
#include <stddef.h>

/*
1780번이랑 유사한문제
*/
int paper[200][200];
int what_color[2];
void cut(int x,int y,int n) {
    int a = paper[x][y];
    int T = true;
    if (n == 1) {
        what_color[a]++;
        return;
    }
    for (int i = 0; i < n; i++) {//1.같은색인정사각형확인
        for (int j = 0; j < n; j++) {
            if (a != paper[x + i][y + j]) {
                T = false;
                break;
            }
        }
        if (T == false)
            break;
    }
    if (T == true) {
        what_color[a]++;
        return;
    }
    //만약 색이섞여잇다면
    int w = n / 2;
    cut(x, y, w);
    cut(x + w, y, w);
    cut(x, y + w, w);
    cut(x + w, y + w, w);
}

int main()
{
    int n;

    scanf("%d", &n);

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            scanf("%d", &paper[i][j]);
        }
    }
    cut(0, 0, n);
    
    printf("%d\n", what_color[0]);
    printf("%d\n", what_color[1]);

    return 0;
}
