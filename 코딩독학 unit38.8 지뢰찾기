#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>    // srand, rand 함수가 선언된 헤더 파일
#include <time.h>      // time 함수가 선언된 헤더 파일
#include <string.h>

int main()
{
    int m, n;
    scanf("%d %d", &m, &n);

    char** matrix = malloc(sizeof(char*) * m);

    for (int i = 0; i < m; i++) {
        matrix[i] = malloc(sizeof(char) * (n+1));
    }
    //메모리 할당

    for (int i = 0; i < m; i++)
    {
        scanf("%s", matrix[i]);
    }

    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            if (matrix[i][j] != '*') {//만약 지뢰가 아닌부분이라면
                int c = 0;

                for (int k = -1; k <= 1; k++) {
                    for (int l = -1; l <= 1; l++) {
                        if (i + k >= 0 && j + l >= 0 && i + k < m && j + l < n) {
                            if (matrix[i + k][j + l] == '*')
                                c += 1;
                        }
                    }
                }//주변 지뢰 탐색절차

                matrix[i][j] = c + '0';
            }
            
        }
    }


    for (int i = 0; i < m; i++) {
        printf("%s\n", matrix[i]);
   }
    

    //메모리 해제

    for (int i = 0; i < m; i++) {
        free(matrix[i]);
    }
    free(matrix);


    return 0;
}
