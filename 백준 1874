#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>    // srand, rand 함수가 선언된 헤더 파일
#include <string.h>
#include <stdbool.h>
#include <stddef.h>
#include <math.h>

int dat[1000000];
int pos = 0;
char text[200000];
int in = 1;

void push(int x) {
    dat[pos++] = x;
    text[in++] = '+';
}

void pop() {
    pos--;
    text[in++] = '-';
}

int main()
{
    int n;
    scanf("%d", &n);
    int s[100000] ;

    for (int i = 0; i < n; i++) {
        scanf("%d", &s[i]);
    }

    text[0] = '+';//처음은 푸시가 보장됨
    dat[pos] = 1;
    pos++;
    int j = 2;
    int i = 0;
    int tru = 1;
    while (i < n)
    {    
        if (pos == 0) {
            push(j);
            j++;
        }
        else if (pos >= 1 && s[i] > dat[pos - 1]) {
            push(j);
            j++;
        }
        else if (pos >= 1 && s[i] == dat[pos - 1]) {
            pop();
            i++;
        }
        else if (pos >= 1 && s[i] < dat[pos - 1]) {
            tru = 0;
            break;
        }

    }
    
    if (tru == 0) {
        printf("NO");
    }
    else {
        for (int k = 0; k < in; k++) {
            printf("%c\n", text[k]);
        }
    }
    return 0;
    
}



